class Student(Person):
	def __init__(self,firstName,lastName,idNumber,scores):
		Person.__init__(self,firstName,lastName,idNumber,scores)
		
	def calculation(self):
		sum=0
		
		for score in scores:
			sum+=score
		a=sum/len(scores)
		if 90<=a<=100:
			return 'O'
		elif 80<=a<=90:
			return 'E'
		elif 70<=a<=80:
			return 'A'
		elif 55<=a<=70:
			return 'P'
		elif 40<=a<=55:
			return 'D'
		elif a<40:
			return 'T'
			
				
				
				
	
